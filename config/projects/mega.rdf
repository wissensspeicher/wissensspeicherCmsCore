  <!-- mandatory: Name of project -->
  <foaf:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marx-Engels-Gesamtausgabe</foaf:name>
  <!-- abstract description of project -->
  <dcterms:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die "Marx-Engels-Gesamtausgabe" (MEGA) ist gemäß den 1992 revidierten Editionsrichtlinien die vollständige, historisch-kritische Ausgabe der Veröffentlichungen, der Handschriften und des Briefwechsels von Karl Marx (1818–1883) und Friedrich Engels (1820–1895). Sie gliedert sich in vier Abteilungen: Erste Abteilung: Werke. Artikel. Entwürfe; Zweite Abteilung: "Das Kapital" und Vorarbeiten; Dritte Abteilung: Briefwechsel; Vierte Abteilung: Exzerpte. Notizen. Marginalien.</dcterms:abstract>
  <!-- mandatory: Main homepage (URL) of the project -->
  <foaf:homepage rdf:resource="http://mega.bbaw.de"/>
  <!-- main language of project (ISO 639-3 code with 3 letters): e.g. ger, eng, fra, lat, ara -->
  <dc:language><dcterms:ISO639-3><rdf:value>ger</rdf:value></dcterms:ISO639-3></dc:language>
  <!-- project type: possible values: Vorhaben, Drittmittelprojekt, interdisziplinäre Arbeitsgruppe, Initiative -->
  <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vorhaben</dc:description>
  <!-- mandatory: possible values: aktiv, abgeschlossen -->
  <foaf:status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">aktiv</foaf:status>
  <!-- start and end of the project (from, to): e.g. start=2007; end=2012; name=Laufzeit -->
  <dcterms:valid rdf:datatype="http://purl.org/dc/terms/Period">start=1999; name=Laufzeit</dcterms:valid>
  <!-- coverage of the project: possible values: http://wsp.normdata.rdf/Gegenwart, http://wsp.normdata.rdf/Neuzeit, http://wsp.normdata.rdf/Mittelalter, http://wsp.normdata.rdf/Antike  -->
