    <foaf:name>Georg Gustav Erbkam: Tagebuch meiner egyptischen Reise</foaf:name>
    <foaf:homepage rdf:resource="http://aaew.bbaw.de/archive/erbkam-tagebuch"/>
    <dcterms:isPartOf rdf:resource="http://wsp.normdata.rdf/AltaegyptWB"/>
