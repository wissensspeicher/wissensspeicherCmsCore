		<dc:contributor rdf:resource="http://wsp.normdata.rdf/Huettl"/>
		<gnd:topic rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wissenschaftsforschung</gnd:topic>
		<foaf:nick rdf:datatype="http://www.w3.org/2001/XMLSchema#string">gwt</foaf:nick>
	</rdf:Description>
