		<dcterms:isPartOf rdf:resource="http://wsp.normdata.rdf/ZentrumAlteWelt"/>
		<dc:contributor rdf:resource="http://wsp.normdata.rdf/Peter"/>
		<foaf:homepage rdf:resource="http://gmw.bbaw.de"/>
