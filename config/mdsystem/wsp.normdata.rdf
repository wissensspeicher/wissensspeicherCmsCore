    <!-- http://wsp.normdata.rdf/ProduktionssystemeZuk√ºnftige -->

    <rdf:Description rdf:about="http://wsp.normdata.rdf/ProduktionssystemeZuk√ºnftige">
        <rdf:type rdf:resource="&foaf;Project"/>
        <dcterms:valid rdf:datatype="&dcterms;Period">start=1994; end=1997; name=Laufzeit</dcterms:valid>
