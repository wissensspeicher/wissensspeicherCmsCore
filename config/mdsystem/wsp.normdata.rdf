	<rdf:Description rdf:about="http://wsp.normdata.rdf/DWB">
    <dc:language>ger</dc:language>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Project"/>
		<dc:contributor rdf:resource="http://wsp.normdata.rdf/Schrader"/>
