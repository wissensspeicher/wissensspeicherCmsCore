    <rdf:Description rdf:about="http://wsp.normdata.rdf/GesellschaftWasserTechnik">
        <rdf:type rdf:resource="&foaf;Project"/>
        <dc:subject>Technikforschung</dc:subject>
        <foaf:status>aktiv</foaf:status>
