        <dc:contributor rdf:resource="http://wsp.normdata.rdf/SchmidtS"/>
        <foaf:fundedBy rdf:resource="http://wsp.normdata.rdf/UEK"/>
        <dc:contributor rdf:resource="http://wsp.normdata.rdf/Virmond"/>
