		<dc:contributor rdf:resource="http://wsp.normdata.rdf/Meyer"/>
		<dcterms:valid rdf:datatype="http://purl.org/dc/terms/Period">start=1996; end=2006; name=Laufzeit</dcterms:valid>
		<dc:contributor rdf:resource="http://wsp.normdata.rdf/Stenger"/>
