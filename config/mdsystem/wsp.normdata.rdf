        <dc:contributor rdf:resource="http://wsp.normdata.rdf/Kohli"/>
        <gnd:contributingCorporateBody rdf:resource="http://wsp.normdata.rdf/Leopoldina"/>
        <dc:format rdf:resource="http://wsp.normdata.rdf/Zukunftsfragen"/>
        <foaf:homepage rdf:resource="http://www.bbaw.de/forschung/Fertilitaet"/>
    </rdf:Description>
    


