	<rdf:Description rdf:about="http://wsp.normdata.rdf/DTM">
    <dc:language>ger</dc:language>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Project"/>
		<dcterms:valid rdf:datatype="http://purl.org/dc/terms/Period">end=2012-12; name=Laufzeit</dcterms:valid>
